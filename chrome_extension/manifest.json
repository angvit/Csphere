{
  "manifest_version": 3,
  "name": "Csphere",
  "version": "1.0",
  "description": "Easily save, organize, enrich, and semantically search over your tabs.",
  "permissions": [
    "tabs",
    "identity",
    "storage",
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "images/Csphere-icon-16.png",
    "48": "images/Csphere-icon-48.png",
    "128": "images/Csphere-icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/Csphere-icon-128.png"
  },
  "homepage_url": "https://csphere-nly9.vercel.app/",
  "host_permissions": [
    "http://localhost:3000/",
    "http://127.0.0.1:3000/",
    "http://localhost:8000/",
    "http://127.0.0.1:8000/",
    "https://pacific-citadel-30742-a7291f1f6593.herokuapp.com/",
    "https://csphere-nly9.vercel.app/"
  ]
}