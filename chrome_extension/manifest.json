{
  "manifest_version": 3,
  "name": "Csphere Tab Saver",
  "version": "1.0",
  "key": "",
  "description": "Google chrome extension that works alongside our app to bookmark their tabs.",
  "permissions": [
    "tabs",
    "cookies",
    "identity",
    "identity.email",
    "storage",
    "activeTab",
    "scripting"
  ],

  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/Logo.png"
  },

  "host_permissions": [
    "http://localhost:3000/",
    "http://127.0.0.1:3000/",
    "http://localhost:8000/",
    "http://127.0.0.1:8000/",
    "https://pacific-citadel-30742-a7291f1f6593.herokuapp.com/",
    "https://csphere-nly9.vercel.app/"
  ]
}
